{
    "template": "sv_warningAcknowledge_warning_notice",
    "description": "When viewing acknowledge warning inside notice censor warning title",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": ": $warning.title",
    "replace": ": $warning.hasOption('hasCensoredTitle') ? $warning.title_censored : phrase('n_a')"
}